{% extends 'base.html.twig' %}

{% block body_id 'user_signup' %}

{% block main %}
    <h1>{{ 'title.signup_user'|trans }}</h1>

    {{ form_start(form) }}
        {{ form_widget(form) }}

        <button type="submit" class="btn btn-primary">
            <i class="fa fa-save" aria-hidden="true"></i> {{ 'action.save'|trans }}
        </button>
    {{ form_end(form) }}
{% endblock %}

{% block sidebar %}
    <div class="section">
        <div class="well well-lg">
            <p>
                {{ 'help.welcome'|trans|raw }}
            </p>
        </div>
    </div>
    {# parent() #}
    {#{{ }}#}
    {# show_source_code(_self)#}
    {#{{ }}#}

{% endblock %}
